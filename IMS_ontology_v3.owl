# http://www.AccessLearner.org/accessible_ocw#Learner_1

:Learner_1 a owl:NamedIndividual , :Learner ;
	:accessModeReq :visual_textual ;
	:adaptationTypeReq :visual_alternativeText , :visual_audioDiscription ;
	:atInteroperable "true"^^xsd:boolean ;
	:languageOfAdaptation "Spanish"^^xsd:string ;
	:languageOfInterface "Spanish"^^xsd:string ;
	GenericOntology:Meet "Lena was born blind. She completed her high</br>school but she couldn't find a job." ;
	GenericOntology:TechnologyUsage "Lena uses her PC (Windows XP) on a daily basis to check her email and apply for jobs. She uses the Jaws screenreader and she also</br>uses OCR software, a Braille embosser and Adobe reader speech output. "^^xsd:string ;
	GenericOntology:User_has_Disability :Blindness ;
	GenericOntology:hasAge "20"^^xsd:int ;
	GenericOntology:hasEducation "High school"^^xsd:string ;
	GenericOntology:hasJob "Unemployed"^^xsd:string ;
	GenericOntology:hasLocation "Spain"^^xsd:string ;
	GenericOntology:hasMaritalStatus "Single"^^xsd:string ;
	GenericOntology:hasName "Lena Fox"^^xsd:string .
